                            oncology-ontology:celulas "cutaneas" ,
                                                      "queratinocitos" ;
                            oncology-ontology:desechos_metabolicos "agua" ,
                                                                   "sales" ;
                            oncology-ontology:ganglios "linfaticos perifericos" ;
                            oncology-ontology:glandulas "exocrinas" ;
                            oncology-ontology:organo_descricion "órgano más grande del cuerpo, que actúa como barrera protectora. Regula la temperatura, permite la sensación y ayuda en la producción de vitamina D. Además, protege contra infecciones y deshidratación." ;
                            oncology-ontology:organo_nombre "Piel" ;
                            oncology-ontology:pertenece oncology-ontology:Sistema_04 .


###  http://www.semanticweb.org/ontologies/oncology-ontology#Organo_08
oncology-ontology:Organo_08 rdf:type owl:NamedIndividual ,
